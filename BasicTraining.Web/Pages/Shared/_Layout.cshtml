<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - BasicTraining.Web</title>

    <link async rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">

    <link async rel="stylesheet" href="~/css/site.min.css" asp-append-version="true" />

    @await this.RenderSectionAsync("Heads", false)
</head>
<body>

    <div class="container">
        @this.RenderBody()
    </div>

    @await this.RenderSectionAsync("Scripts", false)
</body>
</html>
